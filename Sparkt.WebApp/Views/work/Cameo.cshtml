
@{
    ViewBag.Title = "Sparkt Work | Cameo";
    Layout = "~/Views/Shared/_LayoutWork.cshtml";
    ViewBag.MetaTitle = "Sparkt | Cameo";
    ViewBag.MetaDescription = "Alternate Entertainment";
    ViewBag.MetaURL = "https://sparkt.in/work/cameo";
    ViewBag.MetaImage = "https://sparkt.in/Content/work/images/thums/cameo-thum.jpg";
}

    <main>
        <div class="section">

            <div class="work-banner">
                <div class="banner-source">
                    <div class="banner-video middle-align" style="background-image: url('/Content/work/images/banners/cameo-cover.jpg');"> </div>
                </div>
                <div class="head-title floated ">
                    <i class="title-icon"><img src="/Content/work/images/title-icon.png" alt="" /></i>
                    <h1>Cameo</h1>
                    <div class="clear"></div>
                    <span>Alternate Entertainment </span>
                </div>
            </div>

            @Html.Partial("_WorkMenu")
        </div>
        <div class="section">
            <div class="head-title floated ">
                <i class="title-icon"><img src="/Content/work/images/title-icon.png" alt="" /></i>
                <h2>Creative Assets</h2>
            </div>
            <div class="creative-assets-wrapp">
                <div class="assets-carousel fixed cameo-bg">
                    <div class="slider">
                        <div>
                            <div class="creative-assets ">
                                <div class="container">
                                    <div class="assets-title ">
                                        <h3>Lean in Experience bringing together creators & viewers</h3>
                                    </div>
                                    <div class="project fadeIn" data-t-show="1">
                                        <div class="project__card"> <a class="project__image"><img src="/Content/work/images/cameo-viewer.png" alt=""></a> </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
