<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="UTF-8">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/work/images/fevicon.png">
    <meta name="author" content="Sparkt | A Creative & Digital Marketing Agency in Mumbai, India">
    <meta name="keywords" content="digital agency, creative agency, SMO, SEO, SEM, programmatic media buying, analytics, website design, website development">
    <meta name="description" content="@ViewBag.MetaDescription">
    <!-- Twitter Card data -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="Sparkt">
    <meta name="twitter:title" content="@ViewBag.MetaTitle">
    <meta name="twitter:description" content="@ViewBag.MetaDescription">
    <meta name="twitter:creator" content="Sparkt">
    <meta name="twitter:image" content="@ViewBag.MetaImage" />
    <!-- Open Graph data -->
    <meta property="og:title" content="@ViewBag.MetaTitle" />
    <meta property="og:url" content="@ViewBag.MetaURL" />
    <meta property="og:description" content="@ViewBag.MetaDescription" />
    <meta property="og:site_name" content="Sparkt" />
    <meta property="og:type" content="website" />
    <meta property="fb:admins" content="306588899709911" />
    <meta property="og:image" content="@ViewBag.MetaImage" />
    <!-- Schema.org markup for Google+ -->
    <meta itemprop="name" content="@ViewBag.MetaTitle">
    <meta itemprop="description" content="@ViewBag.MetaDescription">
    <!-- CSS -->
    <link type="text/css" rel="stylesheet" media="screen,projection" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link type="text/css" rel="stylesheet" media="screen,projection" href="~/Content/work/css/reset.css" />
    <link type="text/css" rel="stylesheet" media="screen,projection" href="~/Content/work/css/bundle.css" />
    <link type="text/css" rel="stylesheet" media="screen,projection" href="~/Content/work/plugins/lightgallery/css/lightgallery.css" />
    <link type="text/css" rel="stylesheet" media="screen,projection" href="~/Content/work/plugins/lightgallery/css/lg-transitions.min.css" />
    <link type="text/css" rel="stylesheet" media="screen,projection" href="~/Content/plugins/malihu-custom-scrollbar/css/jquery.mCustomScrollbar.css" />
    <link type="text/css" rel="stylesheet" media="screen,projection" href="~/Content/work/css/style.css" />
    <script type="application/ld+json">
        {
        "@@context": "http://schema.org",
        "@@type": "Organization",
        "url": "http://www.sparkt.in/",
        "logo": "https://www.sparkt.in//Content/images/spakt.png"
        }
    </script>
    <script type="application/ld+json">
        {
        "@@context": "http://schema.org",
        "@@type": "Organization",
        "url": "http://www.sparkt.in/",
        "name": "Sparkt",
        "sameAs": [
        "https://www.facebook.com/sparktindia",
        "https://twitter.com/sparktindia",
        "https://in.linkedin.com/company/sparkt-india",
        "https://www.youtube.com/c/SparktIndia"
        ]
        }
    </script>
    <script type="application/ld+json">
        {
        "@@context": "http://schema.org",
        "@@type": "PostalAddress",
        "streetAddress": "408, Satellite Gazebo, Andheri-Ghatkopar Link Road, Chakala, Andheri (East), Mumbai, Maharashtra 400093",
        "addressLocality": "Andheri",
        "postalCode": "400093",
        "addressCountry": "INDIA"
        }
    </script>
</head>
<body class="@RenderSection("BodyClass", false)">
    <header>
        <a href="/" class="sparkt-logo"> <img src="~/Content/work/images/spaktLogo.png" alt="Sparkt" /> <img src="~/Content/work/images/spaktLogo-text.png" alt="Sparkt" class="sparkt-text" /> </a>
        <div class="hamburger">
            <div class="line-menu half start"></div>
            <div class="line-menu"></div>
            <div class="line-menu half end"></div>  
        </div>
        <div class="main-menu">
            <ul class="scroll-content">
                <li><a href="/"><span>Home</span></a> </li>
                <li><a href="/#we"><span>We</span></a> </li>
                <li><a href="/#clients"><span>Clients</span></a> </li>
                <li><a href="/#work"><span>Work</span></a> </li>
                <li><a href="/#sparktlive"><span>Sparkt Live</span></a> </li>
                <li><a href="/#contact"><span>Get In Touch</span></a> </li>
            </ul>
        </div>
    </header>

    <div class="se-pre-con">
        <div class="loader">
            <div class="bar1"></div>
            <div class="bar2"></div>
            <div class="bar3"></div>
            <div class="bar4"></div>
            <div class="bar5"></div>
            <div class="bar6"></div>
        </div>
    </div>



    @RenderBody()


    <footer>

        <div class="we-love-brands">

            <div class="text-anim">
                <p>We <i class="fa fa-heart hvr-bounce-in"></i></p>
                <p>
                    <span class="word ">Brands</span>
                    <span class="word ">Data</span>
                    <span class="word ">Craft</span>
                    <span class="word ">Change</span>
                    <span class="word ">People</span>
                    <span class="word ">Culture</span>
                    <span class="word ">Conversations</span>
                </p>
            </div>

        </div>
        <div class="footer-nav">
            <ul>
                <li>
                    <a href="@Url.Action("Index", "Home")" class="hvr-trim">
                        <img src="~/Content/work/images/home.jpg" alt="" />
                        <span class="caption">Home</span>

                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Home")?section=We" class="hvr-trim">
                        <img src="~/Content/work/images/we.jpg" alt="" />
                        <span class="caption">We</span>

                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Home")?section=clients" class="hvr-trim">
                        <img src="~/Content/work/images/future.jpg" alt="" />
                        <span class="caption">Clients</span>

                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Home")?section=Work" class="hvr-trim">
                        <img src="~/Content/work/images/work.jpg" alt="" />
                        <span class="caption">Work</span>

                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Home")?section=SparktLive" class="hvr-trim">
                        <img src="~/Content/work/images/sparhtLive.jpg" alt="" />
                        <span class="caption">Sparkt Live</span>

                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Home")?section=Contact" class="hvr-trim">
                        <img src="~/Content/work/images/contact.jpg" alt="" />
                        <span class="caption">Get In Touch</span>

                    </a>
                </li>
            </ul>
        </div>
        <div class="footer-band">
            <div class="social-links">
                <ul>
                    <li><a href="https://www.facebook.com/sparktindia" target="_blank" class="hvr-rectangle-out facebook"><i class="fa fa-facebook-f"></i></a></li>
                    <li><a href="http://www.twitter.com/sparktindia" target="_blank" class="hvr-rectangle-out twitter"><i class="fa fa-twitter"></i></a></li>
                    <li><a href="https://www.youtube.com/c/SparktIndia" target="_blank" class="hvr-rectangle-out youtube"><i class="fa fa-youtube"></i></a></li>
                    <li><a href="https://plus.google.com/+SparktIn" target="_blank" class="hvr-rectangle-out google"><i class="fa fa-google-plus"></i></a></li>
                </ul>
            </div>
            <div class="revent-logo"><!--<span><img src="images/revent-logo.png" alt="Revent"/></span>--></div>
            <div class="copyright"><span>Copyright © 2017-18 Sparkt Pvt. Ltd.</span></div>

        </div>

    </footer>
    <script>
        (function (i, s, o, g, r, a, m) {
