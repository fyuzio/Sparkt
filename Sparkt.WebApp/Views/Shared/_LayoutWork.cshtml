    </script>
    <noscript>
        <img height="1" width="1"
             src="https://www.facebook.com/tr?id=159355768081545&ev=PageView
&noscript=1" style="position: fixed; z-index: -2; visibility: hidden; opacity: 0;  " />
    </noscript>
    <!-- End Facebook Pixel Code -->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
