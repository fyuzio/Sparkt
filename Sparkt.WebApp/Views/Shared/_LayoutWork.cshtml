    @*<script type="text/javascript" src="~/Content/work/js/jquery-3.1.1.min.js"></script>*@
    <script type="text/javascript" src="~/Content/plugins/malihu-custom-scrollbar/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script type="text/javascript" src="~/Content/work/plugins/mediaelementplayer/js/mediaelement-and-player.min.js"></script>
    <script type="text/javascript" src="~/Content/work/plugins/slick/js/slick.min.js"></script>
    <script type="text/javascript" src="~/Content/work/plugins/scrollmagic/js/lib/greensock/TweenMax.min.js"></script>
    <script type="text/javascript" src="~/Content/work/plugins/scrollmagic/js/ScrollMagic.min.js"></script>
    <script type="text/javascript" src="~/Content/work/plugins/scrollmagic/js/plugins/animation.gsap.min.js"></script>
    <script type="text/javascript" src="~/Content/work/plugins/hover3d/js/jquery.hover3d.min.js"></script>
    <script type="text/javascript" src="~/Content/work/plugins/lightgallery/js/lightgallery-all.min.js"></script>
    <script type="text/javascript" src="~/Content/plugins/tabs-accordion/js/easy-responsive-tabs.js"></script>
    <script type="text/javascript" src="~/Content/work/plugins/tooltipster/js/tooltipster.bundle.js"></script>
    <script type="text/javascript" src="~/Content/work/js/text-animation.js"></script>
    <!--<script type="text/javascript" src="plugins/scrollmagic/js/plugins/debug.addIndicators.min.js" ></script>-->
    <script type="text/javascript" src="~/Content/work/js/functions.js"></script>
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            $('.se-pre-con').delay(1700).fadeOut('slow');
        });
    </script>
</body>
</html>
